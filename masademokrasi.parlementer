                        body = JSON.stringify(body)
            url = infoObj.engine.url;
        } else if (infoObj.engine.type == 'api-3') {
            url = infoObj.engine.url;
        }

        return new Promise((resolve, reject) => {
            let xhr = new XMLHttpRequest();
            xhr.open(infoObj.engine['method'], url);
